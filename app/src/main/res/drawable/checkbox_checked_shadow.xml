<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape android:shape="oval"

            >

            <size
                android:width="139dp"
                android:height="136dp" />
            <padding
                android:bottom="30dp"
                android:left="20dp"
                android:right="20dp"
                android:top="10dp" />


            <gradient
                android:angle="270"
                android:centerColor="#05FFFFFF"
                android:centerX=".5"
                android:centerY=".5"
                android:endColor="#05FFFFFF"
                android:gradientRadius="100dp"
                android:startColor="#F643E4C3"
                android:type="radial" />

        </shape>
    </item>


    <item android:drawable="@drawable/checkbox_checked" />


</layer-list>